{{#each nav.before}}
    <section-title><a href="{{ relative_path ../current path }}">{{ title }}</a></section-title>
{{/each}}
{{#each nav.groups}}
    {{#if name}}
    <section-list-title>{{ name }}</section-list-title>
    {{/if}}
    <ul>
        {{#each items}}
        <li><a href="{{ relative_path ../../current path }}">{{ title }}</a></li>
        {{/each}}
    </ul>
{{/each}}
{{#each nav.after}}
    <section-title><a href="{{ relative_path ../current path }}">{{ title }}</a></section-title>
{{/each}}